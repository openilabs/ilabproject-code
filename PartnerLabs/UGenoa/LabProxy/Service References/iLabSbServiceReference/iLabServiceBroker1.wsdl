      <wsdl:output message="tns:RetrieveExperimentRecordsSoapOut" />
    </wsdl:operation>
    <wsdl:operation name="RequestExperimentAccess">
      <wsdl:documentation xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/">Uses the cridentials granted the experiment specified by the opHeader to check access to the requested experiment, if allowed a new ticket collection is started to access the requested experiment and optional ESS records. Returns null if access denied.</wsdl:documentation>
      <wsdl:input message="tns:RequestExperimentAccessSoapIn" />
      <wsdl:output message="tns:RequestExperimentAccessSoapOut" />
    </wsdl:operation>
